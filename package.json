{
  "name": "@jclem/effect-llm",
  "version": "0.0.6",
  "description": "An Effect wrapper around some LLM APIs",
  "license": "MIT",
  "author": "Jonathan Clem <j@jclem.me> (https://jclem.me)",
  "homepage": "https://github.com/jclem/effect-llm",
  "keywords": [
    "effect",
    "ai",
    "llm"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclem/effect-llm.git"
  },
  "bugs": "https://github.com/jclem/effect-llm/issues",
  "type": "module",
  "scripts": {
    "build": "pkgroll --sourcemap --clean-dist",
    "publish-package": "bun run build && npm publish"
  },
  "exports": {
    "./generate": {
      "import": {
        "types": "./dist/generate.d.ts",
        "default": "./dist/generate.mjs"
      },
      "require": {
        "types": "./dist/generate.d.ts",
        "default": "./dist/generate.cjs"
      }
    },
    "./thread": {
      "import": {
        "types": "./dist/thread.d.ts",
        "default": "./dist/thread.mjs"
      },
      "require": {
        "types": "./dist/thread.d.ts",
        "default": "./dist/thread.cjs"
      }
    },
    "./providers/anthropic": {
      "import": {
        "types": "./dist/providers/anthropic.d.ts",
        "default": "./dist/providers/anthropic.mjs"
      },
      "require": {
        "types": "./dist/providers/anthropic.d.ts",
        "default": "./dist/providers/anthropic.cjs"
      }
    },
    "./providers/openai": {
      "import": {
        "types": "./dist/providers/openai.d.ts",
        "default": "./dist/providers/openai.mjs"
      },
      "require": {
        "types": "./dist/providers/openai.d.ts",
        "default": "./dist/providers/openai.cjs"
      }
    }
  },
  "dependencies": {
    "@effect/platform": "^0.58.2",
    "@effect/schema": "^0.68.5",
    "@jclem/assert": "^3.1.0",
    "effect": "^3.4.0",
    "eventsource-parser": "^1.1.2"
  },
  "devDependencies": {
    "@effect/platform-bun": "^0.38.1",
    "@types/bun": "latest",
    "pkgroll": "^2.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
